<h1>Listing Category</h1>
<table class="table table-bordered table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Status</th>
    </tr>
  </thead>
  <tbody id="categories">
    <% Category.all.each do |category| %>
    <%= render "categories/category", :category => category %>
    <% end %>
  </tbody>
</table>

<%#= will_paginate(@categories) %>